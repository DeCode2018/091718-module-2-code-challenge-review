<h1>Orders#index</h1>
<table>
  <tr>
    <th>Customer Name</th>
    <th>Item Name</th>
    <th>Quantity</th>
  </tr>
  <% @orders.each do |order| %>
    <tr>
      <td><%=order.customer.full_name%></td>
      <td><%=order.item.name%></td>
      <td><%=order.quantity%></td>
      <td><%=link_to :show, order_path(order)%></td>
      <td><%=link_to :edit, edit_order_path(order)%></td>
    </tr>
  <% end %>
</table>

<br><br>

<%= link_to :new, new_order_path %>
